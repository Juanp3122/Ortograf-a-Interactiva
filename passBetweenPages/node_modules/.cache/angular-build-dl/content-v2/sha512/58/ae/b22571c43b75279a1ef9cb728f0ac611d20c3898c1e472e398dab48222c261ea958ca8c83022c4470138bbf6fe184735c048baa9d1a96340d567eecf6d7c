          value: function inicializar() {
            this.numAciertos = 0;
            this.numFallos = 0;
            this.fallos = [];
            this.Aciertos = [];
            var numero = Math.floor(Math.random() * this.PALABRAS.length);
            this.palabraAAdivinar = this.PALABRAS[numero];
            this.generarPalabraAdivinadaPorAhor();
          }
        }, {
          key: "generarPalabraAdivinadaPorAhor",
          value: function generarPalabraAdivinadaPorAhor() {
            this.palabraAdivinadaPorAhora = "";

            for (var i = 0; i < this.palabraAAdivinar.length; i++) {
              this.palabraAdivinadaPorAhora += "-";
            }
          }
        }, {
          key: "botonClickend",
          value: function botonClickend(letra) {
            if (!this.letraAcertada(letra)) {
              if (this.numFallos < 6) {
                this.aumentarFallos(letra);
              }

              if (this.numFallos == 6) {
                this.mostrarMensajeDePerder();
                console.log(this.numFallos);
              }
            } else {
              var aciertos = this.Aciertos.toString();

              for (var i = 0; i < aciertos.length; i++) {
                aciertos = aciertos.replace(",", "");
              }

              if (this.numAciertos == this.palabraAAdivinar.length && aciertos == this.palabraAAdivinar) {
                this.mostrarMensajeDeGanar();
                console.log(this.numFallos);
              }
            }
          }
        }, {
          key: "aumentarFallos",
          value: function aumentarFallos(letra) {
            this.fallos.push(letra);
            this.numFallos++;
          }
        }, {
          key: "letraAcertada",
          value: function letraAcertada(letra) {
            var letraAcertada = false;
            var longitud = this.palabraAAdivinar.length;

            for (var i = 0; i < longitud; i++) {
              if (letra == this.palabraAAdivinar[i]) {
                this.palabraAdivinadaPorAhora = (i == 0 ? "" : this.palabraAdivinadaPorAhora.substr(0, i)) + letra + this.palabraAdivinadaPorAhora.substr(i + 1);
                letraAcertada = true;
                this.numAciertos++;
                this.Aciertos[i] = letra;
              }
            }

            return letraAcertada;
          }
