\\\">\\r\\n            \\r\\n                                            <h3>Incorrecto!, {{palabra[indice]}} no es {{respuesta}} por que no {{setRetroalimentacion()}} </h3>\\r\\n                                        </div>\\r\\n                                    </ion-list>\\r\\n                            <ion-list class=\\\"btn\\\">\\r\\n                                <ion-button color=\\\"secondary\\\" (click)=\\\"siguiente()\\\">Siguiente</ion-button>\\r\\n                                <ion-label style=\\\"margin-left:30px; font-size:20px \\\">Puntaje :{{ puntaje}}</ion-label>\\r\\n                            </ion-list>\\r\\n        \\r\\n                        </ion-list>\\r\\n                    </ion-card-content>\\r\\n\\r\\n                </ion-card >\\r\\n                <ion-card  *ngIf=\\\"!setContenido()\\\" color=\\\"light\\\"  style=\\\"width: 100%;\\\">\\r\\n                    <ion-card-content>\\r\\n                        <ion-list class=\\\"btn\\\">\\r\\n                            <ion-label style=\\\"font-size: 75px;align-self: center;\\\">Tu puntaje es: {{ puntaje}}/100 </ion-label>\\r\\n                        </ion-list>\\r\\n                        <ion-list class=\\\"btn\\\">\\r\\n                            <ion-button   \\r\\n                                    (click)=\\\"Retry()\\\">¡Vuelve a intentarlo!</ion-button>\\r\\n                        </ion-list>\\r\\n                    </ion-card-content>\\r\\n\\r\\n                </ion-card>\\r\\n\\r\\n            </ion-col>\\r\\n            \\r\\n        </ion-row>\\r\\n\\r\\n\\r\\n    </ion-grid>\\r\\n</ion-content>\";","import { NgModule } from '@angular/core';\r\nimport { Routes, RouterModule } from '@angular/router';\r\n\r\nimport { ActividadesPage } from './actividades.page';\r\n\r\nconst routes: Routes = [\r\n  {\r\n    path: '',\r\n    component: ActividadesPage\r\n  }\r\n];\r\n\r\n@NgModule({\r\n  imports: [RouterModule.forChild(routes)],\r\n  exports: [RouterModule],\r